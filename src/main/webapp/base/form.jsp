<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head>
    <title>Title</title>
</head>
<body>
<form action="">
    <label>Name</label>
    <input type="text" placeholder="your name">
    <br><br>
    <label>Email</label>
    <input type="email" placeholder="Email">
    <br><br>
    <label>Password</label>
    <input type="password" placeholder="Password">
    <br><br>
    <label>Checkbox</label>
    <input type="checkbox" placeholder="checkbox1">Checkbox 1
    <input type="checkbox" placeholder="checkbox1">Checkbox 2
    <br><br>
    <label>Radio</label>
    <input type="radio"  name="g">male
    <input type="radio" name="g">female
    <br><br>
    <label>number</label>
    <input type="number">
    <br><br>
    <label>range</label>
    <input type="range" min="0" max="100" value="50">
    <br><br>
    <label>Date</label>
    <input type="date">
    <br><br>
    <label>Time</label>
    <input type="time">
    <br><br>
    <label>file</label>
    <input type="file">
    <br><br>
    <label>color</label>
    <input type="color">
    <br><br>
    <label>textarea</label>
    <textarea cols="30" rows="10"></textarea>
    <br><br>
    <label>select</label>
    <select name="">
        <option value="nepal">Nepal</option>
        <option value="nepal">China</option>
        <option value="nepal">India</option>
        <option value="nepal">Japan</option>
    </select>
    <br><br>

    <input type="submit" value="Submit Data">
    <button type="reset">Reset</button>
</form>

</body>
</html>
